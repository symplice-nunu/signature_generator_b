# Ignore target folder, where build artifacts are stored
/target/

# Ignore Maven and Gradle directories
.mvn/
.gradle/
build/

# Ignore dependency caching
**/target/**/*.jar
**/target/**/*.war
**/target/**/*.ear
**/target/**/*.class

# Ignore IDE-specific folders
# IntelliJ
.idea/
*.iml
*.iws
# Eclipse
.project
.classpath
.settings/
# NetBeans
nbproject/private/
nbbuild/
dist/
nbdist/
.nb-gradle/

# Ignore OS-specific files
# macOS
.DS_Store
.AppleDouble
.LSOverride
# Windows
Thumbs.db
ehthumbs.db
Desktop.ini
# Linux
*~

# Ignore logs
logs/
*.log
**/logs/

# Ignore temporary files
*.tmp
*.swp
*.swo
*.bak
*.orig

# Ignore compiled outputs
*.class

# Ignore application properties/secrets
application*.yml
application*.yaml
application*.properties
.env

# Ignore other specific files
### Common security files
secrets/
*.key
*.pem
*.p12
*.jks
*.keystore

# Ignore other artifacts generated by build tools
out/
!src/main/resources/
